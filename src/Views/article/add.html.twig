{% extends "layout.html.twig" %}

{% block body %}
    <h1>Ajouter un article</h1>
    <form method="post">
        <label for="title">Titre</label>
        <input id="title" class="form-control" type="text" name="titleArticle">

        <label for="content">Contenu</label>
        <textarea id="content" class="form-control" name="contentArticle" cols="30" rows="10"></textarea>

        <input class="form-control btn btn-primary" type="submit" name="addArticle">
    </form>
{% endblock %}